{
  "productName": "TunnelForge",
  "version": "1.0.0",
  "identifier": "dev.tunnelforge.linux",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "copyright": "Copyright Â© 2024 TunnelForge Team",
    "category": "DeveloperTool",
    "shortDescription": "Terminal sharing made simple",
    "longDescription": "TunnelForge allows you to share terminal sessions through any web browser with a beautiful, fast desktop interface.",
    "linux": {
      "deb": {
        "depends": ["libwebkit2gtk-4.0-37", "libgtk-3-0", "libayatana-appindicator3-1"]
      },
      "appimage": {
        "bundleMediaFramework": false
      }
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://releases.tunnelforge.dev/linux/{{current_version}}/{{target}}/{{arch}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJBMUY0QkU5RkQ4NEUyOUEKUldSWXR0L3VyRWY3TXE4dFJhWkZOS3FzZWFJS2tCdnVuNzN2SFhBaDhJbzE5RVoxU2RmZzNKZwoK"
    }
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "TunnelForge",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "visible": false,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
