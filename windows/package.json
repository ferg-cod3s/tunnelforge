{
  "name": "tunnelforge-windows",
  "version": "1.0.0",
  "description": "TunnelForge Desktop App for Windows - Terminal sharing with web interface",
  "author": "TunnelForge Team",
  "license": "MIT",
  "homepage": "https://tunnelforge.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/ferg-cod3s/tunnelforge.git",
    "directory": "windows"
  },
  "scripts": {
    "dev": "tauri dev",
    "build": "tauri build",
    "build:debug": "tauri build --debug",
    "build:msi": "tauri build --target msi",
    "build:nsis": "tauri build --target nsis",
    "build:exe": "tauri build --target exe",
    "clean": "cargo clean && rm -rf dist node_modules",
    "setup": "cargo install tauri-cli && npm install",
    "check": "cargo check",
    "clippy": "cargo clippy",
    "fmt": "cargo fmt"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}